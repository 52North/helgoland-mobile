{
  "name": "helgoland-mobile",
  "version": "0.0.1",
  "author": "Jan Schulte",
  "homepage": "http://52north.org/",
  "private": true,
  "config": {
    "ionic_bundler": "webpack",
    "ionic_copy": "./config/copy.config.js",
    "ionic_webpack": "./config/webpack.config.js"
  },
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "start": "npm run ionic:serve",
    "start:android:device": "ionic cordova run android --device -lc",
    "build": "ionic-app-scripts build",
    "build:android:prod": "ionic cordova build android --prod",
    "build:android:prod:release": "ionic cordova build android --prod --release",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "build-netlify": "npm run build && cp ./netlify/* ./www -r",
    "i18n-extract": "ngx-translate-extract -i ./src -o ./src/assets/i18n/*.json --clean --sort --format namespaced-json"
  },
  "dependencies": {
    "@angular/common": "^6.0.5",
    "@angular/compiler": "^6.0.5",
    "@angular/compiler-cli": "^6.0.5",
    "@angular/core": "^6.0.5",
    "@angular/forms": "^6.0.5",
    "@angular/http": "^6.0.5",
    "@angular/platform-browser": "^6.0.5",
    "@angular/platform-browser-dynamic": "^6.0.5",
    "@helgoland/caching": "0.0.1-alpha.53",
    "@helgoland/core": "0.0.1-alpha.53",
    "@helgoland/depiction": "0.0.1-alpha.53",
    "@helgoland/favorite": "0.0.1-alpha.53",
    "@helgoland/flot": "0.0.1-alpha.53",
    "@helgoland/map": "0.0.1-alpha.53",
